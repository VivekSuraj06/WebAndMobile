{
  "author": "vivek Suraj",
  "name": "cucumber-boilerplate",
  "version": "2.0.0",
  "description": "DailyMail QA test with Appium/web/browserStack Integration",
  "type": "module",
  "scripts": {
    "web-test": "wdio run configs/web.wdio.conf.ts --cucumberOpts.tags @webTest",
    "android-test": "wdio run configs/android.wdio.conf.ts --cucumberOpts.tags @androidTest",
    "iOS-test": "wdio run configs/android.wdio.conf.ts --cucumberOpts.tags @iosTest",
    "bs-web": "wdio run configs/browserStackWeb.wdio.conf.ts --cucumberOpts.tags @webTest",
    "bs-and": "wdio run configs/browserStackAndroid.wdio.conf.ts --cucumberOpts.tags @androidTest",
    "bs-iOS": "wdio run configs/browserStackIOS.wdio.conf.ts --cucumberOpts.tags @iOSTest"
  },
  "dependencies": {
    "@tsconfig/node18": "1.0.3",
    "@wdio/appium-service": "^8.27.0",
    "@wdio/cli": "^8.26.2",
    "@wdio/config": "^8.26.2",
    "@wdio/cucumber-framework": "^8.26.2",
    "@wdio/globals": "^8.26.2",
    "@wdio/local-runner": "^8.26.2",
    "@wdio/shared-store-service": "^8.27.0",
    "@wdio/spec-reporter": "^8.26.2",
    "@wdio/static-server-service": "^8.26.2",
    "appium": "^2.3.0",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "webdriverio": "^8.26.2"
  },
  "devDependencies": {
    "@wdio/browserstack-service": "^8.27.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "wdio-html-nice-reporter": "^8.1.3-beta"
  }
}
